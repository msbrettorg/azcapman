{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.cs" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "src": "docs",
        "dest": "."
      },
      {
        "files": [ "api/*.yml", "api/*.md" ],
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "src": "docs"
      }
    ],
    "output": "_site",
    "template": [
      "default"
    ],
    "globalMetadata": {
      "_appTitle": "ISV Quota & Capacity Management",
      "_appName": "Azure ISV Documentation",
      "_appLogoPath": "images/azure-logo.svg",
      "_appFooter": "Copyright Â© 2024 Microsoft Corporation",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_disableDarkMode": true,
      "_defaultTheme": "light",
      "_gitContribute": {
        "repo": "https://github.com/msbrett/azcapman",
        "branch": "main"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [ "ExtractSearchIndex" ],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}